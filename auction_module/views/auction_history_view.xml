<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>



        <record id="view_auction_history_tree" model="ir.ui.view">
            <field name="name">auction.history.tree</field>
            <field name="model">auction.history</field>
            <field name="arch" type="xml">

                <tree string="History" create="0" edit="0">
                    <field name="create_date"/>
                    <field name="team_id"/>
                    <field name="message"/>
                    <field name="tournament_id"/>
                </tree>
            </field>
        </record>

        <record id="action_auction_history_tree" model="ir.actions.act_window">
            <field name="name">Auction History</field>
            <field name="res_model">auction.history</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>
